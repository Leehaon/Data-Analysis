        "# 3. 여러 요약 통계량 한 번에 구하기\n",
        "exam.groupby('nclass') \\\n",
        "  .agg(mean_math = ('math', 'mean'),\n",
        "       sum_math = ('math', 'sum'),\n",
        "       mediam_math = ('math', 'median'),\n",
        "       n = ('nclass', 'count'))"
