      "source": [
        "# 결측치 제거 후 분석\n",
        "df.dropna(subset = ['s', 'score']) \\\n",
        ".groupby('s') \\\n",
        ".agg(mean_score = ('score', 'mean'))"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 143
